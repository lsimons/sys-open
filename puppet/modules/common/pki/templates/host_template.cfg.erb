# PUPPET
# Generated by puppet.
# This is used to create certificates for hosts and services
organization = <%= pki['ca_org'] %>
unit = "<%= pki['ca_unit'] %>"
locality = <%= pki['ca_locality'] %>
state = "<%= pki['ca_state'] %>"
country = "<%= pki['ca_country'] %>"
cn = <%= fqdn %>
tls_www_server
tls_www_client
encryption_key
signing_key
expiration_days = <%= pki['client_expiration_days'] %>
serial = <%= serial %>
honor_crq_extensions
