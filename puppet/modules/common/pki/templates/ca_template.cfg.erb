# PUPPET
# Generated by puppet.
# This is used to create the initial Certificate Authority
organization = "<%= pki['ca_org'] %>"
unit = "<%= pki['ca_unit'] %>"
locality = <%= pki['ca_locality'] %>
state = "<%= pki['ca_state'] %>"
country = "<%= pki['ca_country'] %>"
cn = "<%= clientcert %>"
serial = 1
expiration_days = <%= pki['ca_expiration_days'] %>

# X.509 v3 extensions
crl_dist_points = "<%= pki['ca_crl_dist_point'] %>"
ca
#tls_www_client
#tls_www_server
signing_key
#encryption_key
cert_signing_key
crl_signing_key
#code_signing_key
#ocsp_signing_key
#time_stamping_key
#ipsec_ike_key
#key_purpose_oids = "1.3.6.1.5.5.7.3.1" "1.2.3.4.5.6"
